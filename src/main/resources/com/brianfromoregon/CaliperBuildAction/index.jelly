<j:jelly xmlns:j="jelly:core" xmlns:st="jelly:stapler" xmlns:d="jelly:define" xmlns:l="/lib/layout"
         xmlns:t="/lib/hudson" xmlns:f="/lib/form">
    <l:layout>
        <st:include it="${it.build}" page="sidepanel.jelly"/>
        <l:main-panel>

            <j:forEach items="${it.results}" var="i">
                <p>${i.toString()}</p>
            </j:forEach>
        </l:main-panel>
    </l:layout>
</j:jelly>
